<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.brilliant.sean.justdoit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NumOne"
            android:label="First listview item clicked activity"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".NumTwo"
        android:label="Second listview item clicked activity"
        android:theme="@style/AppTheme"
        />
        <activity android:name=".NumThree"
            android:label="Third listview item clicked activity"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".NumFour"
            android:label="Fourth listview item clicked activity"
            android:theme="@style/AppTheme"
            />
        <activity android:name=".NumFive"
            android:label="Fifth listview item clicked activity"
            android:theme="@style/AppTheme"
            />
    </application>

</manifest>
